{
  "name": "@dsts/data-structures-typescript",
  "version": "0.0.1",
  "description": "Data Structures in TypeScript",
  "main": "build/data-structures.js",
  "scripts": {
    "build": "npm run build:linkedlist",
    "build:linkedlist": "tsc -p ./packages/linked-list/tsconfig.linkedlist.json",
    "chore:delete-changelog-branch": "if git show-ref --quiet refs/heads/chore-changelog; then git branch -D chore-changelog; fi",
    "chore:branch": "git checkout -b chore-changelog",
    "chore:changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "chore:setup-next-work": "git checkout master && npm run chore:delete-changelog-branch",
    "chore:pr": "git add . && git commit -m '[chore] updates changelog' --no-verify && git push origin chore-changelog -f",
    "chore:setup-changelog": "git checkout master && git pull",
    "chore": "npm run chore:delete-changelog-branch && npm run chore:branch && npm run chore:changelog && npm run chore:pr && npm run chore:setup-next-work",
    "grammar": "write-good *.md --no-passive",
    "lint": "npm run lint:grammar && npm run lint:md && npm run lint:ts",
    "lint:grammar": "write-good ./packages/**/*.md --no-passive",
    "lint:md": "markdownlint *.md",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts}'",
    "markdownlint": "markdownlint *.md",
    "prepush": "npm run build && npm test",
    "pre-commit-msg": "Echo 'Running pre-commit checks...' && exit 0",
    "postpublish": "git tag $npm_package_version && git push origin --tags && npm run chore",
    "start": "nvm i && npm i",
    "test": "npm run test:unit",
    "test:unit": "jest --coverage"
  },
  "repository": "git+https://github.com/yowainwright/datastructures-ts.git",
  "keywords": [
    "TypeScript",
    "TS",
    "JavaScript",
    "DataStructures",
    "Data",
    "Structures"
  ],
  "author": "Jeff Wainwright <yowainwright@gmail.com> (https://jeffry.in)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yowainwright/datastructures-ts/issues"
  },
  "homepage": "https://github.com/yowainwright/datastructures-ts#readme",
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "conventional-changelog-cli": "^2.0.11",
    "debug": "^4.1.0",
    "es-check": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "markdown-spellcheck": "^1.3.1",
    "markdownlint-cli": "^0.13.0",
    "rollup": "^0.66.6",
    "ts-jest": "^23.10.4",
    "tslint": "^5.9.1",
    "tslint-config-dollarshaveclub": "^1.0.2",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6",
    "write-good": "^0.13.1"
  },
  "jest": {
    "testURL": "https://localhost/",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "quokka": {
    "babel": true
  }
}
